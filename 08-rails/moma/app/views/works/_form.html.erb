<%= form_for @work do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :year %>
    <%= f.text_field :year %>

    <%= f.label :medium %>
    <%= f.text_field :medium %>

    <%= f.label :style %>
    <%= f.text_field :style %>

    <%= f.label :image %>
    <%= f.url_field :image %>

    <%= f.label :artist_id %>
    <%= f.select :artist_id, Artist.pluck(:name, :id), :include_blank => true %>

    <%= f.submit %>
<% end %>
